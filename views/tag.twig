{% extends "base.html" %}

{% block content %}
  <h1>tag</h1>
  <img src="{{src}}" alt="" width="250" height="250" />
  <form method="post" action="/viztag/tag">
    <fieldset>
      <legend>add tags</legend>
      <input type="hidden" name="vs_id" value="{{id}}" />
      <input class="input-xxlarge" type="text" id="viztag-tagging" placeholder="enter tags..." autocomplete="off" name="tags">
    </fieldset>
  </form>
{% endblock %}
